(set-logic QF_UFBV)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const x (_ BitVec 8))
(declare-const y (_ BitVec 8))
(define-fun xlow () (_ BitVec 4) ((_ extract 3 0) x))
(define-fun ylow () (_ BitVec 4) ((_ extract 3 0) y))
(define-fun xhigh () (_ BitVec 4) ((_ extract 7 4) x))
(define-fun yhigh () (_ BitVec 4) ((_ extract 7 4) y))
(declare-fun f ((_ BitVec 8)) (_ BitVec 8))
(assert (! (distinct (f x) (f y)) :name diff))
(assert (! (= xlow ylow) :name equal-low))
(check-sat)
(assert (! (= xhigh yhigh) :name equal-high))
(check-sat)
(exit)